account_key: <%= @account_key %>
hostname: <%= @hostname || '' %>
display_name: <%= @display_name || '' %>
log_file: <%= @log_file || '' %>
ruby_path: <%= @ruby_path || '' %>
environment: <%= @environment || '' %>
<% if @roles && !@roles.empty -%>
roles: <% @roles.each do |role| -%>
  - <%= @role %>
<% end -%>
<% end -%>
http_proxy: <%= @http_proxy || '' %>
https_proxy: <%= @https_proxy || '' %>
